{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Note Here",
  "description": "Note Here is a simple note extension. It can be sync across devices through the Chrome's storage API.",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "permissions": [
    "storage"
  ],
  
  "action": {
    "default_popup": "popup.html"
  }
}
